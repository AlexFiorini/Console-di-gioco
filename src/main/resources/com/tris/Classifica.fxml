<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.tris.Classifica"
            prefHeight="80.0" prefWidth="200.0">
    <children>
        <GridPane>
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="50.0" vgrow="SOMETIMES" />
            </rowConstraints>
            <children>
                <Label fx:id="l1" prefHeight="30.0" prefWidth="100.0" text="X" alignment="CENTER" />
                <Label fx:id="l2" prefHeight="30.0" prefWidth="100.0" text="O" GridPane.columnIndex="1" alignment="CENTER"/>
                <Label fx:id="lX" prefHeight="50.0" prefWidth="100.0" text="a" GridPane.rowIndex="1" alignment="CENTER" />
                <Label fx:id="lO" prefHeight="50.0" prefWidth="100.0" text="b" GridPane.columnIndex="1" GridPane.rowIndex="1" alignment="CENTER" />
            </children>
        </GridPane>
    </children>


</AnchorPane>
